<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="demo" package="net.azrafe7.geomAlgo" version="1.0.0" company="azrafe7" />
	<app main="OpenflDemo" path="bin" file="OpenflDemo" />

	<window background="#222222" fps="60" />
	<window width="900" height="650" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />

	<haxeflag name="-D" value="swf-script-timeout=60" />

	<set name="SWF_VERSION" value="11.3" />
	<source path="src" />

	<haxelib name="openfl" />
	<haxelib name="hxPixels" />

	<assets path="assets" rename="assets" exclude="openfl.svg" include="*.png" />
	<icon path="assets/openfl.svg" />

</project>